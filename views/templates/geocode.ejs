
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css">

   <!-- Modal -->
  <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        
        <div class="modal-header">
          <form id="location-form">
            <input type="text" id="location-input" placeholder="Ingrese una dirección" class="form-control form-control-lg">   
            <button type="submit" class="btn btn-outline-success ">Buscar</button>
            <button id="btn_guardar_info" class="btn btn-outline-success" onclick="getGoogleResponse()" data-dismiss="modal" aria-label="Close">Guardar Ubicación</button> 
          </form> 
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        </div>
        

        <div class="modal-body">
          <div class="row">
            <div class="col-md-12 modal_body_map">
              <div class="location-map" id="location-map">
                <div style="width: 600px; height: 400px;" id="map_canvas"></div>
              </div>
            </div>
          </div>

        </div>
      </div>
    </div>
  </div>



